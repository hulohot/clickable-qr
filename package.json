{
    "name": "qr-code-tooltip",
    "version": "1.0.0",
    "description": "Chrome extension that makes QR codes clickable and shows their content in tooltips",
    "scripts": {
        "build": "webpack --config webpack.config.js",
        "watch": "webpack --watch --config webpack.config.js",
        "serve": "http-server -p 8080 -c-1",
        "dev": "npm-run-all --parallel watch serve",
        "package": "npm run build && node build.js"
    },
    "dependencies": {
        "jsqr": "^1.4.0",
        "qrcode": "^1.5.4"
    },
    "devDependencies": {
        "archiver": "^5.3.1",
        "http-server": "^14.1.1",
        "npm-run-all": "^4.1.5",
        "webpack": "^5.97.1",
        "webpack-cli": "^5.1.4"
    },
    "private": true,
    "author": "Your Name",
    "license": "MIT",
    "homepage": "https://github.com/yourusername/qr-code-tooltip#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yourusername/qr-code-tooltip.git"
    },
    "bugs": {
        "url": "https://github.com/yourusername/qr-code-tooltip/issues"
    },
    "keywords": [
        "chrome-extension",
        "qr-code",
        "tooltip"
    ]
}
